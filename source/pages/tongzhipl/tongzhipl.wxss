Page {
  background: #F9F9F8;
}
.margin30 {
  margin: 20rpx 30rpx;
}
.touxiang {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}

.huifu{
  width: 100rpx;
  height: 40rpx;
  background: #FFFFFF;
  border: 1rpx solid #FF6900;
  border-radius: 20rpx;
  color: #FF6900;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
}
.huifu2 {
  width: 100rpx;
  height: 40rpx;
  background: #FFFFFF;
  border: 1rpx solid #999999;
  border-radius: 20rpx;
  color: #999999;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: center;
}
.cons {
  width: 600rpx;
  min-height: 100rpx;
  background: #F9F9F8;
  border-radius: 10rpx;
}
.community_img {
  width: 100rpx;
  height: 100rpx;
}
.paddingcon {
  padding: 24rpx 28rpx 24rpx 20rpx;
}
.lines{
  width: 100%;
  height: 1rpx;
  background: #EEEEEE;
  margin-top: 30rpx;
  margin-bottom: 10rpx;
}

.inputs{
  padding: 22rpx 20rpx;
  background: #ffffff;
  border-radius: 35rpx;
  margin-right: 18rpx;
}
.fixedbottom {
  width: 100vw;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
}
